<body>
    <header>
        <nav class="navlist" >
            <div class="log"></div>
            <div class="menu" >
                <div class="list">
                    <ul>
                        <li class="active"><a href="/"><i class="fa-regular fa-house"></i></a></li>
                    </ul>                   
                    <ul><li>
                        <% if(locals.me.success ){ %>
                            <% if(locals.me.success.attachement != "") {%> 
                            <a  href="/profile">
                                <img src="/img/uploads/<%= locals.me.success.attachement %>" alt="image-profil" class="imgprofile" >
                            </a>
                        <% }else{ %>
                            <a href="/profile" class="avatar"><%= locals.me.success.username %></a>
                            <% } %>
                        <% } %>
                    </li>
                    </ul>
                    <ul>
                        <li><a href="/logout"><i class="fa-light fa-arrow-right-from-bracket"></i></a></li>
                    </ul>
                </div>
                <div class="menuburger"><i class="fa-solid fa-bars"></i></div>
            </div>
        </nav>      
    </header>     
<br>